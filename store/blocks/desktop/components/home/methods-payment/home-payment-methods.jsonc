      "flex-layout.col#home__payment--six"
    ],
    "props": {
      "blockClass": "home__payment--methods"
    }
  },
  "flex-layout.col#home__payment--one": {
    "title": "Home easy buy col1",
    "children": [
      "image#img__buy",
      "rich-text#text__buy",
      "rich-text#text__decription__buy"
    ],
    "props": {
      "blockClass": "home__payments--col"
    }
  },

  "image#img__buy": {
    "title": "Image easy-buy",
    "props": {
      "src": "https://cosonyb2c.vteximg.com.br/arquivos/como-comprar.png",
      "blockClass": "image__payment--size"
    }
  },

  "rich-text#text__buy": {
    "title": "TEXT EASY BUY",
    "props": {
      "text": "Compra fácil",
      "blockClass": "text__orange--payment"
    }
  },

  "rich-text#text__decription__buy": {
