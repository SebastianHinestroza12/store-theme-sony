{
  "flex-layout.row#home__payment--methods": {
    "title": "Home__payment--methods",
    "children": [
      "flex-layout.col#home__payment--one",
      "flex-layout.col#home__payment--two",
      "flex-layout.col#home__payment--three",
      "flex-layout.col#home__payment--four",
